{
    "name": "genealabs/laravel-model-caching",
    "description": "Automatic caching for Eloquent models.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "fico7489/laravel-pivot": "*",
        "illuminate/cache": "5.5 - 5.6",
        "illuminate/config": "5.5 - 5.6",
        "illuminate/console": "5.5 - 5.6",
        "illuminate/database": "5.5 - 5.6",
        "illuminate/support": "5.5 - 5.6",
        "php": ">=7.1.3"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "*",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "orchestra/database": "3.5.*",
        "orchestra/testbench-browser-kit": "3.5.*",
        "orchestra/testbench": "3.5.*",
        "php-coveralls/php-coveralls" : "*",
        "phpmd/phpmd": "*",
        "phpunit/phpunit": "*",
        "predis/predis": "^1.1",
        "sebastian/phpcpd": "*",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelModelCaching\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/database/factories"
        ],
        "psr-4": {
            "GeneaLabs\\LaravelModelCaching\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelModelCaching\\Providers\\Service"
            ]
        }
    }
}
